    "compilerOptions": {
        "baseUrl": ".",
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es6",
        "jsx": "react",
        "sourceMap": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "noImplicitAny": true,
        "types": ["webpack-env"]
    },
    "exclude": [
        "bin",
        "node_modules"
    ]
}